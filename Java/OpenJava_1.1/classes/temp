for(int i = 0; i < methods.length; i++) {

			StatementList varInit1 = makeStatementList("double temp = 0.0;");
			StatementList varInit2 = makeStatementList("int someInt = 0;");

			varInit1.addAll(methods[i].getBody()); // an ArrayList; addAll appends to end of list
			varInit1.addAll(varInit2);
			methods[i].setBody(varInit1);

		}

		
		
		
		String text = methods[i].toString();
				
				if(text.contains("main")){
					StatementList entering = makeStatementList("System.out.println( \"Entering main...\" );");
					entering.addAll(methods[i].getBody()); // an ArrayList; addAll appends to end of list
					methods[i].setBody(entering);
				}
				else if(text.contains("foo")){
					if(text.contains("A")){
						StatementList entering = makeStatementList("System.out.println( \"Entering A::foo...\" );");
						StatementList leaving = makeStatementList("System.out.println( \"Leaving A::foo.\" );");
						entering.addAll(methods[i].getBody()); // an ArrayList; addAll appends to end of list
						entering.addAll(leaving);
						methods[i].setBody(entering);
					}
					else if(text.contains("B")){
						StatementList entering = makeStatementList("System.out.println( \"Entering B::foo...\" );");
						StatementList leaving = makeStatementList("System.out.println( \"Leaving B::foo.\" );");
						entering.addAll(methods[i].getBody()); // an ArrayList; addAll appends to end of list
						entering.addAll(leaving);
						methods[i].setBody(entering);
					}
					else if(text.contains("C")){
						StatementList entering = makeStatementList("System.out.println( \"Entering C::foo...\" );");
						StatementList leaving = makeStatementList("System.out.println( \"Leaving C::foo.\" );");
						entering.addAll(methods[i].getBody()); // an ArrayList; addAll appends to end of list
						entering.addAll(leaving);
						methods[i].setBody(entering);
					}
				}
				else if(text.contains("bar")){
					if(text.contains("A")){
						StatementList entering = makeStatementList("System.out.println( \"Entering A::bar...\" );");
						StatementList leaving = makeStatementList("System.out.println( \"Leaving A::bar.\" );");
						entering.addAll(methods[i].getBody()); // an ArrayList; addAll appends to end of list
						entering.addAll(leaving);
						methods[i].setBody(entering);
					}
					else if(text.contains("B")){
						StatementList entering = makeStatementList("System.out.println( \"Entering B::bar...\" );");
						StatementList leaving = makeStatementList("System.out.println( \"Leaving B::bar.\" );");
						entering.addAll(methods[i].getBody()); // an ArrayList; addAll appends to end of list
						entering.addAll(leaving);
						methods[i].setBody(entering);
					}
					else if(text.contains("C")){
						StatementList entering = makeStatementList("System.out.println( \"Entering C::bar...\" );");
						StatementList leaving = makeStatementList("System.out.println( \"Leaving C::bar.\" );");
						entering.addAll(methods[i].getBody()); // an ArrayList; addAll appends to end of list
						entering.addAll(leaving);
						methods[i].setBody(entering);
					}
				}